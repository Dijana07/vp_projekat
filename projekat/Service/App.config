<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="rejectCSV" value="rejects.csv"/>
		<add key="validCSV" value="measurements_session.csv"/>
		<add key="T_threshold" value="10"/>
		<add key="RH_threshold" value="5"/>
		<add key="DEW_threshold" value="2"/>
		<add key="DataDirectory" value=".\Data"/>
		<add key="DeviationThreshold" value="0.25"/>
	</appSettings>
	<system.serviceModel>
		<services>
			<service name="Service.TransferMetaService">
				<endpoint
				  address="MetaTransferingService"
				  binding="netTcpBinding"
				  bindingConfiguration="StreamedNetTcpBinding"
				  contract="Common.ITransferMeta" />
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4002" />
					</baseAddresses>
				</host>
			</service>
		</services>
		<bindings>
			<netTcpBinding>
				<binding name="StreamedNetTcpBinding"
						 transferMode="Streamed"
						 maxReceivedMessageSize="10000000"
						 sendTimeout="00:10:00"
						 receiveTimeout="00:10:00"
						 closeTimeout="00:01:00"
						 openTimeout="00:01:00">
				</binding>
			</netTcpBinding>
		</bindings>
	</system.serviceModel>
</configuration>