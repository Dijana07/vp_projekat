<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="rejectCSV" value="reject.csv"/>
		<add key="validCSV" value="valid.csv"/>
		<add key="startCSV" value="start.csv"/>
	</appSettings>
	<system.serviceModel>
		<services>
			<service name="Service.TransferMetaService">
				<endpoint
				  address="MetaTransferingService"
				  binding="netTcpBinding"
				  contract="Common.ITransferMeta" />
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:4002" />
					</baseAddresses>
				</host>
			</service>

		</services>
	</system.serviceModel>
</configuration>