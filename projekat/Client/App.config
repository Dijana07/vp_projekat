<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="startCSV" value=".\Data\start.csv"/>
		<add key="logFile" value=".\Logs\session.log"/>
	</appSettings>
	<system.serviceModel>
		<client>
			<endpoint name="TransferMetaService"
					  address="net.tcp://localhost:4002/MetaTransferingService"
					  binding="netTcpBinding"
					  bindingConfiguration="StreamedNetTcpBinding"
					  contract="Common.ITransferMeta" />
		</client>
		<bindings>
			<netTcpBinding>
				<binding name="StreamedNetTcpBinding"
						 transferMode="Streamed"
						 maxReceivedMessageSize="10000000"
						 sendTimeout="00:10:00"
						 receiveTimeout="00:10:00"
						 closeTimeout="00:01:00"
						 openTimeout="00:01:00">
				</binding>
			</netTcpBinding>
		</bindings>
	</system.serviceModel>
</configuration>