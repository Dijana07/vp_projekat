<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<appSettings>
		<add key="rejectCSV" value="reject.csv"/>
		<add key="validCSV" value="valid.csv"/>
		<add key="startCSV" value="start.csv"/>
	</appSettings>
	<system.serviceModel>
		<client>
			<endpoint name="TransferMetaService"
					  address="net.tcp://localhost:4002/MetaTransferingService"
					  binding="netTcpBinding"
					  contract="Common.ITransferMeta" />
		</client>
	</system.serviceModel>
</configuration>